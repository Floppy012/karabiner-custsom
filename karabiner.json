{
  "title": "Floppy Custom",
  "rules": [
    {
      "description": "Ctrl+Backspace -> CMD+Backspace",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "control"
              ]
            },
            "key_code": "delete_or_backspace"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "delete_or_backspace",
              "modifiers": [
                "left_command"
              ]
            }
          ]
        }
      ]
    }
  ]
}
